html
	head
		title= title
		link(href='/css/styles.css', rel='stylesheet', type='text/css')
	body
		h1= message
		div(class='form')
			form(
				action= formAction
				method= formMethod
			)
				input(
					type= inputTextAreaType
					name= inputTextAreaName
				)
				input(
					type= inputButtonType
					value= inputButtonValue
				)
